<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.apps.finalversion.tokidprojects">

    <application
        android:name="com.apps.finalversion.tokidprojects.fragments.notification.AppNotification"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <receiver
            android:name="com.apps.finalversion.tokidprojects.fragments.notification.NotificationReceiver"
            android:enabled="false"
            android:exported="false"></receiver>

        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.HeightGraphBoyes" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.WeightGraphBoyes" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.HeightGraphMain" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.WeightGraphMain" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.HeightGraph" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.WeightGraph" />
        <activity
            android:name="com.apps.finalversion.tokidprojects.oldversioncode.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.apps.finalversion.tokidprojects.oldversioncode.BoysName"
            android:label="@string/title_activity_boys_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.apps.finalversion.tokidprojects.oldversioncode.GirlsName"
            android:label="@string/title_activity_girls_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.apps.finalversion.tokidprojects.fragments.name.BoyesNameDetails"
            android:label="@string/title_activity_boyes_name_details"
            android:parentActivityName="com.apps.finalversion.tokidprojects.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.apps.finalversion.tokidprojects.fragments.name.GirlsDetails"
            android:label="@string/title_activity_girls_details"
            android:parentActivityName="com.apps.finalversion.tokidprojects.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.apps.finalversion.tokidprojects.oldversioncode.BoysFlowchart"
            android:label="@string/title_activity_boys_flowchart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.GirlsFlowchart" />
        <activity android:name="com.apps.finalversion.tokidprojects.oldversioncode.HelthProfile" />
        <activity
            android:name="com.apps.finalversion.tokidprojects.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.apps.finalversion.tokidprojects.fragments.notification.NotificationService" />


    </application>

</manifest>