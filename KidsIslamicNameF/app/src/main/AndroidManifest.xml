<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.apps.finalversion.tokidprojects">

    <application
        android:name=".fragments.notification.AppNotification"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".activity.BasicActivity"
            android:label="@string/title_activity_basic"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <receiver
            android:name=".fragments.notification.NotificationReceiver"
            android:enabled="false"
            android:exported="false" />

        <activity android:name=".oldversioncode.HeightGraphBoyes" />
        <activity android:name=".oldversioncode.WeightGraphBoyes" />
        <activity android:name=".oldversioncode.HeightGraphMain" />
        <activity android:name=".oldversioncode.WeightGraphMain" />
        <activity android:name=".oldversioncode.HeightGraph" />
        <activity android:name=".oldversioncode.WeightGraph" />
        <activity
            android:name=".oldversioncode.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".oldversioncode.BoysName"
            android:label="@string/title_activity_boys_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".oldversioncode.GirlsName"
            android:label="@string/title_activity_girls_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".fragments.name.BoyesNameDetails"
            android:label="@string/title_activity_boyes_name_details"
            android:parentActivityName=".HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".fragments.name.GirlsDetails"
            android:label="@string/title_activity_girls_details"
            android:parentActivityName=".HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".oldversioncode.BoysFlowchart"
            android:label="@string/title_activity_boys_flowchart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".oldversioncode.GirlsFlowchart" />
        <activity android:name=".oldversioncode.HelthProfile" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".fragments.notification.NotificationService" />
    </application>

</manifest>